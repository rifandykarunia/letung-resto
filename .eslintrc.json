{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "no-console":"off",
        "no-restricted-globals":"off",
        "no-undef":"off",
        "no-underscore-dangle":"off"
    }
}
