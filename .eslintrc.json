{
    "env": {
        "browser": true,
        "es2022": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "no-console":"off",
        "no-restricted-globals":"off",
        "no-undef":"off",
        "no-underscore-dangle":"off",

        "indent": "off",
        "consistent-return": "off",
        "no-prototype-builtins": "off",
        "import/prefer-default-export" : "off",
        "import/no-extraneous-dependencies": "off",
        "prefer-destructuring": "off"
    }
}
